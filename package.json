{
  "name": "github-hyperjump",
  "version": "1.0.0",
  "description": "Create repository_dispatch events on GitHub repos",
  "author": "Jack Moffitt <metajack@fb.com>",
  "license": "Apache-2.0",
  "repostiory": "https://github.com/libra/github-hyperjump.git",
  "homepage": "https://github.com/libra/github-hyperjump",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess 'npm run start'",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.6",
    "@types/node": "^12.12.51",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "smee-client": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "ts-jest": "^26.1.3",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">= 12.12"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
